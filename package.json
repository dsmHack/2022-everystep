{
  "name": "qr-codes",
  "version": "1.0.0",
  "description": "A QR code generator for Every Step's Amanda the Panda program",
  "main": "index.js",
  "watch": {
    "build": "*.ts"
  },
  "scripts": {
    "build": "esbuild index.ts --bundle --outfile=out.js",
    "build-watch": "npm run build && watch -p \"**/*.ts\" -c \"npm run build\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "esbuild": "^0.14.42",
    "typescript": "^4.7.2",
    "watch-cli": "^0.2.3"
  },
  "dependencies": {
    "jspdf": "^2.5.1",
    "qrcode": "^1.5.0",
    "uuid": "^8.3.2"
  }
}
